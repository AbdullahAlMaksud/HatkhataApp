{
  "expo": {
    "name": "Hatkhata",
    "slug": "hatkhata",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/applogo/logo.png",
    "scheme": "hatkhata",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.hatkhata"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "images": "./src/assets/images/applogo/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.hatkhata"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/applogo/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/applogo/splash-icon-dark.png",
          "resizeMode": "contain"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
